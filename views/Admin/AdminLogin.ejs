<body id="mimin" class="dashboard form-signin-wrapper">

  <div class="container">

    <form class="form-signin" action='/admin/login' method="post">
      <div class="panel periodic-login">
        <span class="atomic-number"></span>
        <div class="panel-body text-center">
          <img src="/js/images/logo_blue.png" alt="" />
          <!-- <h1 class="atomic-symbol">Mi</h1> -->
          <p class="atomic-mass">Admin Login</p>
          <!-- <p class="element-name">Login</p> -->

          <i class="icons icon-arrow-down"></i>
          <div class="form-group form-animate-text" style="margin-top:40px !important;">
            <input type="text" class="form-text" name='adminid' required>
            <span class="bar"></span>
            <label>Admin ID</label>
          </div>
          <div class="form-group form-animate-text" style="margin-top:40px !important;">
            <input type="password" class="form-text" name="adminpassword" required>
            <span class="bar"></span>
            <label>Password</label>
          </div>
          <!-- <label class="pull-left">
            <input type="checkbox" class="icheck pull-left" name="checkbox1" /> Remember me
          </label> -->
          <br>

          <% if(!msg) { %>
          <h4 style="color: red">Invalid Credentials</h4>
          <% } else { %>
          <h4>Please Login</h4>
          <% } %>
          </h4>
          <input type="submit" class="btn col-md-12" value="SignIn" />
        </div>
        <!-- <div class="text-center" style="padding:5px;">
                    <a href="forgotpass.html">Forgot Password </a>
                    <a href="reg.html">| Signup</a>
                </div> -->
      </div>
    </form>

  </div>

